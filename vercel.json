{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "continue": true
    },
    {
      "src": "/SHUFFL(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    } 
  ]
}
